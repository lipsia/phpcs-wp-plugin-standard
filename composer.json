{
  "name": "lipsia/phpcs-wp-plugin-standard",
  "version": "1.1.1",
  "description": "A custom phpcs ruleset for WordPress plugin development.",
  "type": "phpcodesniffer-standard",
  "homepage": "https://github.com/lipsia/phpcs-wp-plugin-standard",
  "license": "MIT",
  "keywords": [
    "phpcs", "wordpress", "wordpress-plugin"
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Lipsia Digital GmbH",
      "email": "info@lipsia.digital"
    }
  ],
  "support": {
    "issues": "https://github.com/lipsia/phpcs-wp-plugin-standard/issues",
    "source": "https://github.com/lipsia/phpcs-wp-plugin-standard"
  },
  "require": {
    "php": "^7.3",
    "automattic/phpcs-neutron-standard": "1.6.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "phpcompatibility/phpcompatibility-paragonie": "^1.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  }
}
